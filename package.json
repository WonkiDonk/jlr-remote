{
  "name": "jlr-remote",
  "version": "0.0.0",
  "description": "Module for interacting with the Jaguar Land Rover Remote API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/WonkiDonk/jlr-remote",
    "type": "github"
  },
  "bugs": {
    "url": "https://github.com/WonkiDonk/jlr-remote/issues"
  },
  "author": "William Cowell",
  "license": "MIT",
  "keywords": [
    "jaguar",
    "land rover",
    "incontrol",
    "remote",
    "i-pace",
    "i pace",
    "ipace"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublish": "tsc",
    "auto-test": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "cz-conventional-changelog": "3.3.0",
    "jest": "^29.4.3",
    "jest-ts-auto-mock": "^2.1.0",
    "ts-jest": "^29.0.5",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
