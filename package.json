      }
    },
    "setupFiles": [
      "<rootDir>tests/config.ts"
    ]
  },
  "dependencies": {
    "axios": "^0.21.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "funding": [
    {
      "type": "individual",
      "url": "https://liberapay.com/william-cowell/donate"
    },
    {
      "type": "paypal",
